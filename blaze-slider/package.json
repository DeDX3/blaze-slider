{
  "name": "blaze-slider",
  "version": "0.0.8",
  "description": "Blazing Fast Slider For High Performance Web",
  "main": "dist/blaze-slider.cjs.js",
  "module": "dist/blaze-slider.esm.js",
  "browser": "dist/blaze-slider.min.js",
  "types": "dist/blaze-slider.d.ts",
  "scripts": {
    "publish-patch": "npm version patch && npm run build && npm publish --public",
    "dist-cleanup": "mv dist/blaze-slider.cjs.dev.d.ts dist/blaze-slider.d.ts && rm -f dist/blaze-slider.cjs.prod.d.ts dist/blaze-slider.esm.d.ts dist/blaze-slider.iife.d.ts dist/blaze-slider.min.d.ts",
    "lint": "eslint src/** --fix",
    "test": "jest",
    "build": "npm run lint && rollup -c && npm run dist-cleanup"
  },
  "keywords": [
    "blaze-slider",
    "slider",
    "swiper",
    "carousel",
    "gallery"
  ],
  "author": "Manan Tank",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.4.5",
    "rollup": "^2.62.0",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-eslint-bundle": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.4",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
